{
  "tableName": "Security_Submissions",
  "primaryField": {
    "name": "submission_id",
    "type": "singleLineText"
  },
  "fields": [
    { "name": "email", "type": "email" },
    { "name": "form_id", "type": "singleLineText" },
    { "name": "submitted_at", "type": "dateTime", "options": { "timeZone": "UTC", "is24Hour": true } },
    { "name": "source", "type": "singleSelect", "options": { "choices": [ {"name":"fillout"}, {"name":"manual"} ] } },

    { "name": "owners", "type": "number", "options": { "precision": 0 } },
    { "name": "access", "type": "number", "options": { "precision": 0 } },
    { "name": "ethics", "type": "checkbox", "options": { "icon":"check", "color":"greenBright" } },
    { "name": "ppl", "type": "number", "options": { "precision": 0 } },
    { "name": "sensitive", "type": "checkbox", "options": { "icon":"check", "color":"yellowBright" } },
    { "name": "sensitive_people", "type": "number", "options": { "precision": 0 } },
    { "name": "sensitive_types", "type": "multipleSelects", "options": { "choices": [
      {"name":"privacy"}, {"name":"medical"}, {"name":"genetic"}, {"name":"biometric"}, {"name":"origin"}, {"name":"criminal"}, {"name":"political_beliefs"}, {"name":"financial"}, {"name":"personality_assessment"}, {"name":"legal_privilege"}
    ] } },
    { "name": "biometric_100k", "type": "checkbox", "options": { "icon":"check", "color":"orangeBright" } },
    { "name": "transfer", "type": "checkbox", "options": { "icon":"check", "color":"blueBright" } },
    { "name": "directmail_biz", "type": "checkbox", "options": { "icon":"check", "color":"blueBright" } },
    { "name": "directmail_self", "type": "checkbox", "options": { "icon":"check", "color":"blue" } },
    { "name": "monitor_1000", "type": "checkbox", "options": { "icon":"check", "color":"purple" } },
    { "name": "processor", "type": "checkbox", "options": { "icon":"check", "color":"cyan" } },
    { "name": "processor_large_org", "type": "checkbox", "options": { "icon":"check", "color":"cyanBright" } },
    { "name": "employees_exposed", "type": "checkbox", "options": { "icon":"check", "color":"pink" } },
    { "name": "cameras", "type": "checkbox", "options": { "icon":"check", "color":"pinkBright" } },

    { "name": "score_level", "type": "singleSelect", "options": { "choices": [ {"name":"lone"},{"name":"basic"},{"name":"mid"},{"name":"high"} ] } },
    { "name": "score_reg", "type": "checkbox", "options": { "icon":"check", "color":"green" } },
    { "name": "score_report", "type": "checkbox", "options": { "icon":"check", "color":"red" } },
    { "name": "score_dpo", "type": "checkbox", "options": { "icon":"check", "color":"yellow" } },
    { "name": "score_requirements", "type": "multipleSelects", "options": { "choices": [
      {"name":"worker_security_agreement"}, {"name":"cameras_policy"}, {"name":"consultation_call"}, {"name":"outsourcing_text"}, {"name":"direct_marketing_rules"}
    ] } },

    { "name": "status", "type": "singleSelect", "options": { "choices": [ {"name":"waiting_review"},{"name":"in_review"},{"name":"approved"},{"name":"sent"} ] } },
    { "name": "reviewer", "type": "singleLineText" },
    { "name": "email_subject", "type": "singleLineText" },
    { "name": "email_to", "type": "email" },
    { "name": "email_body_preview", "type": "longText" },
    { "name": "email_body", "type": "longText" },
    { "name": "sent_at", "type": "dateTime", "options": { "timeZone":"UTC", "is24Hour": true } },
    { "name": "scored", "type": "checkbox", "options": { "icon":"check", "color":"green" } },
    { "name": "review_doc_url", "type": "url" },
    { "name": "pdf_url", "type": "url" }
  ]
}

